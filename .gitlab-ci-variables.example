# Variables d'environnement GitLab CI/CD
# À configurer dans GitLab : Settings > CI/CD > Variables
# Copier ce fichier et définir les valeurs réelles

# Registry Docker
CI_REGISTRY=registry.gitlab.com
CI_REGISTRY_USER=gitlab-ci-token
CI_REGISTRY_PASSWORD=<auto-generated>

# SonarQube
SONARQUBE_URL=http://sonarqube.example.com:9000
SONARQUBE_TOKEN=<sonarqube-token>

# Base de données
MYSQL_ROOT_PASSWORD=<strong-password>
MYSQL_DATABASE=dolibarr
MYSQL_USER=dolibarr
MYSQL_PASSWORD=<strong-password>

# Grafana
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=<strong-password>

# Slack (optionnel pour Alertmanager)
SLACK_WEBHOOK_URL=<slack-webhook-url>

# Registry privé (si différent de GitLab Registry)
REGISTRY_URL=<your-registry-url>

